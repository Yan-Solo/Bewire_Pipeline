pipeline {
  agent any

  stages {
    stage ('Compile Stage') {
      steps {
        sh 'expo build'
      }
    }

    stage ('Test Stage') {
      steps {
        withSonarScanner(sonarscanner : 'sonar-runner2.4') {
          sh 'voer sonarScanner uit'
        }
      }
    }

    stage ('Deployement Stage') {
      steps {
        sh 'Deploy'
      }
    }
  }
}
